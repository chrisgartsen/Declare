<tr>
  <td id='project-name'><%= project.name %></td>
  <td id='project-outstanding-amount'><%= project.outstanding_amount %></td>
  <td id='project-total-amount'><%= project.total_amount %></td>
  <td id='project-created-at'><%= project.created_at %></td>
  <td id='project-updated-at'><%= project.updated_at %></td>
  <td>
    <%= link_to project, id: 'show-project' do %>
      <span class='icon'><i class="fa fa-search"></i></span>
    <% end %>
    <%= link_to edit_project_path(project), id: "edit-project-#{project.id}" do %>
      <span class='icon'><i class="fa fa-pencil"></i></span>
    <% end %>
    <%= link_to project, method: :delete, id: "delete-project-#{project.id}" do %>
      <span class='icon'><i class="fa fa-trash"></i></span>
    <% end %>
  </td>
</tr>

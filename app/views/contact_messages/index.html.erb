<% content_for(:breadcrumb) do %>
  <%= breadcrumb('Contact messages' =>'#') %>
<% end %>

<div class='container'>

  <div class='row'>
    <div class='col-md-10 col-md-offset-1'>

      <h1 class='pageheader'>Contact messages</h1>

      <table class='table table-hover'>
        <thead>
          <th>Email</th>
          <th>Message</th>
          <th>Created at</th>
        </thead>
        <tbody>

          <%= render @contact_messages %>

        </tbody>
      </table>

    </div>
  </div>
</div>
